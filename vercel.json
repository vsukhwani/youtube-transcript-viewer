{
  "version": 2,
  "builds": [
    {
      "src": "api/hello.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/transcript_test.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/transcript_v2.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/hello", "dest": "/api/hello" },
    { "src": "/api/transcript_test", "dest": "/api/transcript_test" },
    { "src": "/api/transcript_v2", "dest": "/api/transcript_v2" },
    { "src": "/api/test", "dest": "/api/test" },
    { "src": "/api$", "dest": "/api/index" },
    { "src": "/debug", "dest": "/frontend/debug.html" },
    { "src": "/", "dest": "/frontend/index.html" },
    { "src": "/(.*)\\.(.+)", "dest": "/frontend/$1.$2" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}
