{
  "version": 2,
  "name": "yt-transcript-viewer-vsukhwani",
  "routes": [
    {
      "src": "/api/transcript",
      "dest": "/api/transcript.py"
    },
    {
      "src": "/api/languages", 
      "dest": "/api/languages.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "ENVIRONMENT": "production"
  }
}
